{
  "name": "screeps-clockwork-core",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/screeps-clockwork-core/src",
  "targets": {
    "build": {
      "cache": true,
      "executor": "nx:run-commands",
      "inputs": ["{projectRoot}/src"],
      "outputs": ["{projectRoot}/pkg"],
      "options": {
        "cwd": "packages/screeps-clockwork-core",
        "command": "wasm-pack build --target web --config build.rustflags=[\\'-Ctarget-feature=-reference-types\\',\\'-Ctarget-feature=-multivalue\\',\\'-Ctarget-feature=-sign-ext\\'] -Z build-std=std,panic_abort"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "test": {
      "cache": true,
      "executor": "@monodon/rust:test",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/screeps-clockwork-core"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "lint": {
      "cache": true,
      "executor": "@monodon/rust:lint",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/screeps-clockwork-core"
      }
    }
  },
  "tags": []
}
